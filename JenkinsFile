pipeline {
    agent any
    stages {
        stage('Build test code') {
            steps {
                bat 'mvn clean install -DskipTests'
            }
        }
        stage('Execute test') {
            steps {
                bat 'mvn test'
            }
        }
    }
    post{
         always {
            junit 'target/site/failsafe-reports/*.xml'
        }
    }
}
